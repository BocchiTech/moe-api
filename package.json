{
  "name": "trace.moe-wrapper",
  "version": "0.2.1",
  "description": "Search anime scene using trace.moe API.",
  "main": "./dist/src/cjs/index.js",
  "module": "./dist/src/esm/index.js",
  "types": "./dist/@types/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/@types/index.d.ts",
        "default": "./dist/src/esm/index.js"
      },
      "require": {
        "types": "./dist/@types/index.d.ts",
        "default": "./dist/src/cjs/index.js"
      },
      "default": "./dist/src/cjs/index.js"
    }
  },
  "scripts": {
    "build": "npm run clean && npm run build:esm && npm run build:cjs && tsconfig-to-dual-package",
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "anime",
    "trace.moe",
    "API",
    "wrapper"
  ],
  "author": "SlavyanDesu",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "tsconfig-to-dual-package": "^1.1.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "axios": "^1.2.3",
    "form-data": "^4.0.0"
  }
}
